{
  "name": "@chittyos/chittyconnect",
  "version": "1.0.0",
  "description": "ChittyConnect - GitHub App integration for ChittyOS ecosystem",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.{js,ts,json}'"
  },
  "keywords": [
    "chittyos",
    "github",
    "app",
    "integration",
    "mcp",
    "cloudflare",
    "workers"
  ],
  "author": "ChittyOS",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.0.0",
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "wrangler": "^3.28.0",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
